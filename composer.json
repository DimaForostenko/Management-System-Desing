{
    "name": "dmytro/management",
    "description": "managment",
    "type": "project",
    "require": {
        "php": "8.3.23"
    },
    "autoload": {
       "psr-4": {
        "Database\\": "src/Database/",
        "Routers\\": "src/Routers/",
        "Controllers\\": "src/Controllers/",
        "Models\\": "src/Models/"
    }
    },
    "autoload-dev": {
    "psr-4": {
        "Tests\\": "tests/"
    }
    },
    "authors": [
        {
            "name": "dmytro"
        }
    ], 
     "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "require-dev": {
        "fakerphp/faker": "^1.20",
        "mockery/mockery": "^1.5",
        "phpunit/phpunit": "^12.2"
    }
}
